{
    "defaultSeverity": "error",
    "extends": [
        "tslint:recommended"
    ],
    "jsRules": {},
    "rules": {
        "quotemark": [
            true,
            "single"
        ],
        "arrow-parens": false,
        "interface-name": [
            true,
            "never-prefix"
        ],
        "no-unused-variable": true,
        "no-unused-imports": true,
        "object-literal-sort-keys": false,
        "ordered-imports": [
            false
        ]
    },
    "rulesDirectory": []
}